{
  "eventHandlers": [
    {
      "name": "csv",
      "class": "org.forgerock.audit.events.handlers.csv.CSVAuditEventHandler",
      "config": {
        "logDirectory": "/tmp/audit"
      },
      "events": [
        "access",
        "activity",
        "config",
        "authentication"
      ]
    },
    {
      "name" : "syslog",
      "class" : "org.forgerock.audit.handlers.syslog.SyslogAuditEventHandler",
      "config" : {
        "protocol" : "UDP",
        "host" : "localhost",
        "port" : 514,
        "facility" : "local0",
        "productName" : "OpenAM"
      },
      "events": [
        "access",
        "activity",
        "config",
        "authentication"
      ]
    }
  ]
}